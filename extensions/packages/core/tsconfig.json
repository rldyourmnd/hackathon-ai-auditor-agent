{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Node",
    "declaration": true,
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@extensions/shared": ["../shared/src"],
      "@extensions/shared/*": ["../shared/src/*"]
    }
  },
  "include": ["src"]
}



